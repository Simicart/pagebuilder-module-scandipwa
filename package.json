{
    "name": "scandi-integrate",
    "version": "0.0.1",
    "license": "OSL-3.0",
    "dependencies": {
        "@scandipwa/eslint-config": "0.1.30",
        "@scandipwa/scandipwa": "4.5.1",
        "@scandipwa/scandipwa-scripts": "2.4.14",
        "@tilework/mosaic": "0.1.4",
        "react-intl": "^5.20.6",
        "simi-pagebuilder-react": "^1.2.3",
        "expose-webpack": "file:packages/expose-webpack"
    },
    "scandipwa": {
        "type": "theme",
        "locales": {
            "en_US": true
        },
        "parentTheme": "@scandipwa/scandipwa",
        "extensions": {
            "expose-webpack": true
        }
    },
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build",
        "postinstall": "scandipwa-scripts link",
        "postupdate": "scandipwa-scripts link"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa"
    },
    "proxy": "https://magento.test"
}
